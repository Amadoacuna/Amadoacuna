UND = off
  function Under()
    if UND == off then
      gg.setRanges(gg.REGION_CODE_APP)
    gg.searchNumber('1E-4', gg.TYPE_FLOAT)
    gg.getResults('100000')
    gg.editAll('-4.2', gg.TYPE_FLOAT)
    gg.toast("UNDER ACTIVADO")
    gg.clearResults()
      UND = on
    elseif UND == on then
      gg.setRanges(gg.REGION_CODE_APP)
    gg.searchNumber('-4.2', gg.TYPE_FLOAT)
    gg.getResults('100000')
    gg.editAll('1E-4', gg.TYPE_FLOAT)
    gg.clearResults()
      gg.toast("üöóDESATIVADOüöó")
      UND = off
    end
end

